
public class tester {
	
	public static void main(String[] args) {
		System.out.println("abb".matches("abb?b"));  // Greedy     -> true
		System.out.println("abb".matches("abb??b")); // Reluctant  -> true
		System.out.println("abb".matches("abb?+"));
		System.out.println("abb".matches("(?=abb)"));
	}

}
