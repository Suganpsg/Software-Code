
public class tester {
	
	public static void main(String[] args) {
		System.out.println("abb".matches("abb?b"));  // Greedy     -> true
		System.out.println("abb".matches("abb??b")); // Reluctant  -> true
		System.out.println("".matches("^(?=([0-9]*))$"));
		System.out.println("x".matches("^"
				+ "(?=.)"
//                + "(?=.*[a-z])(?=.*[A-Z])"
//                + "(?=.*[@#$%^&+=])"
//                + "(?=\\S+)"
//                + "\\d"
                + ".{0,2}"
                + "$"
                ));
	}

}
