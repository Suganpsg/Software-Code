				if thisInst.selected(ProliantDriveCage).filter(x => split(x.id, "#").get(0) >= "826690-B21").count >= 1 && thisInst.selected(NVMEnablement).filter(x => split(x.id, "#").get(0) >= "878192-B21").count < 1 then false;
